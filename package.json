{
  "name": "unpkg-server",
  "version": "1.0.0",
  "private": false,
  "description": "The CDN for everything on npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinSheedy/unpkg-server.git"
  },
  "files": [
    "server.js"
  ],
  "main": "server.js",
  "scripts": {
    "build": "rollup -c",
